public class Solution {
    public string solution(string[] seoul)
        {
            string answer = "";
            for (int i = 0; i < seoul.Length; i++)
            {
                if (seoul[i].CompareTo("Kim") == 0)
                {
                    answer = "김서방은" + " " + i + "에 있다";
                }
            }
            return answer;
        }
}
